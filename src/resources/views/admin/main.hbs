<div class="mt-4">
  <div class="container">
    <h1>My Courses</h1>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">#</th>
          <th scope="col">Title</th>
          <th scope="col">Level</th>
          <th scope="col">CreateAt</th>
          <th scope="col"></th>
        </tr>
      </thead>
      {{#each course}}
        <tbody>
          <tr>
            <th>{{sum @index}}</th>
            <td>{{this.title}}</td>
            <td>{{this.level}}</td>
            <td>{{this.createdAt}}</td>
            <td>
              <a
                href="/admin/{{this._id}}/edit"
                type="button"
                class="btn btn-link"
              >Edit</a>
              <a href="" type="button" class="btn btn-link">Delete</a>
            </td>
          </tr>
        </tbody>
      {{/each}}
    </table>
  </div>
</div>